{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${MCP_FILESYSTEM_PATHS}"
      ]
    }
  },
  "mcp": {
    "enabled": true,
    "accessControl": {
      "allowedDirectories": [
        "${MCP_FILESYSTEM_PATHS}"
      ],
      "allowFileOperations": true
    }
  }
}